from pyspark.sql import functions as F
from pyspark.sql.types import ArrayType, DoubleType

s3_path_hotels = "s3a://de-course-bucket-epam1/m06sparkbasics/hotels/"

def get_lat_lng(full_address):
    return [1.0, 1.0]

geo_udf = F.udf(get_lat_lng, ArrayType(DoubleType()))

# ---------- LOAD ----------
df_hotels = (
    spark.read.option("header", "true").csv(s3_path_hotels)
        .withColumn("Latitude", F.col("Latitude").cast("double"))
        .withColumn("Longitude", F.col("Longitude").cast("double"))
)

# ---------- PROCESS ----------
df_hotels = (
    df_hotels
    .withColumn(
        "full_addr",
        F.concat_ws(" ", "Address", "City", "Country")
    )
    .withColumn(
        "geo_new",
        F.when(
            F.col("Latitude").isNull() | F.col("Longitude").isNull(),
            geo_udf(F.coalesce(F.col("full_addr")), "")
        )
    )
    .withColumn("Longitude", F.coalesce("Longitude", F.col("geo_new")[1]))
    .withColumn("Latitude",  F.coalesce("Latitude",  F.col("geo_new")[0]))
    .drop("full_addr", "geo_new")
)
df_hotels.collect()
